<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luis.pruebaas1" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:configChanges="locale"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Facil"
            android:configChanges="locale"
            android:label="@string/facil"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Configuracion"
            android:icon="@drawable/settings_icon"
            android:label="@string/configuracion"
            android:noHistory="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.luis.pruebaas1.MainActivity" />
        </activity>
        <activity
            android:name=".Creditos"
            android:configChanges="locale"
            android:label="@string/creditos"
            android:noHistory="true"
            android:largeHeap="true"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.luis.pruebaas1.MainActivity" />
        </activity>
        <activity
            android:name=".Niveles"
            android:configChanges="locale"
            android:label="@string/nivel"
            android:noHistory="true"
            android:largeHeap="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Medio"
            android:label="@string/medio"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Dificil"
            android:label="@string/dificl"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name=".ScoreFinal"
            android:label="@string/title_activity_final_score"
            android:noHistory="true"
            android:largeHeap="true"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
